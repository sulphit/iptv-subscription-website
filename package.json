{
  "name": "iptv-subscription-website",
  "version": "1.0.0",
  "description": "Professional IPTV subscription selling website",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && node server.js",
    "client": "cd frontend && npm start",
